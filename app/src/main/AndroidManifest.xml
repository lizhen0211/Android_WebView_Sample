<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lz.android_webview_sample">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WebViewClientActivity" />
        <activity android:name=".BasicWebviewActivity" />
        <activity android:name=".WebSettingActivity" />
        <activity android:name=".WebChromeClientActivity" />
        <activity android:name=".advanced.JSCallAndroidByInjectActivity" />
        <activity android:name=".advanced.AndroidCallJsActivity" />
        <activity android:name=".advanced.JSCallAndroidByshouldOverrideUrlLoadingActivity" />
        <activity android:name=".advanced.JSCallAndroidByJsDialogActivity" />
        <activity android:name=".simplejsbridge.SimpleJsBridgeActivity" />
        <activity android:name=".otherjsbridge.OtherJsBridgeActivity" />
    </application>

</manifest>